    "%sql\n",
    "CREATE OR REPLACE TABLE events_silver AS\n",
    "SELECT DISTINCT customer_id, event_type, timestamp\n",
    "FROM events_bronze\n",
    "WHERE event_type IN (\"click\",\"view\",\"purchase\")\n",
    "  AND timestamp RLIKE '^\\\\d{4}-\\\\d{2}-\\\\d{2}T.*Z$';"
